select sum(amount_usd) as total_usd, strftime('%m-%Y ', datetime(timestamp, 'unixepoch', 'utc')) as month from volume_stats where datetime(timestamp, 'unixepoch', 'utc') >= date('now','start of month','-12 months') group by strftime('%m-%Y', datetime(timestamp, 'unixepoch', 'utc')) order by timestamp;